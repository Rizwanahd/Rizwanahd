<%= form_with model: @features do |form| %>
  <%=form.label :"Name:" %><br>
  <%= form.text_field :name %><br>
  <%=form.label :"Description:"%><br>
  <%= form.text_area :description %>
  <%= f.label :"Status" %><br />
  <%= f.select :status, [:New, :"In progress", :"In review", :completed] %>
  <%= form.submit %>
<% end %>